{
  "ignoreDirs": [".git","static_files"],
  "ignoreFiles": [".gitignore"],
  "depManagers": [
    {
      "lang": "Java",
      "depTool": "maven",
      "fileNames": ["pom.xml"],
      "fileExt": ".java"
    },
    {
      "lang": "Java",
      "depTool": "gradle",
      "fileNames": ["build.gradle"],
      "fileExt": ".java"
    },
    {
      "lang": "JavaScript",
      "depTool": "npm",
      "fileNames": ["package-lock.json"],
      "fileExt": ".js"
    },
    {
      "lang": "Python",
      "depTool": "pip",
      "fileNames": ["requirements.txt","requirements"],
      "fileExt": ".py"
    },
    {
      "lang": "Python",
      "depTool": "pipenv",
      "fileNames": ["Pipfile.lock"],
      "fileExt": ".py"
    },
    {
      "lang": "Python",
      "depTool": "poetry",
      "fileNames": ["poetry.lock"],
       "fileExt":".py"
    },
    {
      "lang": "Go",
      "depTool": "mod",
      "fileNames": ["go.sum"],
      "fileExt":".go"
    }
  ]
}
