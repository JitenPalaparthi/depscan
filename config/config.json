{
  "ignoreDirs": [".git","static_files"],
  "ignoreFiles": [".gitignore"],
  "depManagers": [
    {
      "lang": "Java",
      "depTool": "maven",
      "fileNames": ["pom.xml"],
      "fileExt": ".java"
    },
    {
      "lang": "Java",
      "depTool": "gradle",
      "fileNames": ["build.gradle","dependencies.lock"],
      "fileExt": ".java"
    },
    {
      "lang": "JavaScript",
      "depTool": "npm",
      "fileNames": ["package-lock.json","package.json"],
      "fileExt": ".js"
    },
    {
      "lang": "Python",
      "depTool": "pypi",
      "fileNames": ["requirements.txt","requirements"],
      "fileExt": ".py"
    },
    {
      "lang": "Python",
      "depTool": "pipenv",
      "fileNames": ["Pipfile.lock"],
      "fileExt": ".py"
    },
    {
      "lang": "Python",
      "depTool": "poetry",
      "fileNames": ["poetry.lock"],
       "fileExt":".py"
    },
    {
      "lang": "Php",
      "depTool": "composer",
      "fileNames": ["composer.lock"],
      "fileExt":".php"
    }
    ,
    {
      "lang": "Go",
      "depTool": "mod",
      "fileNames": ["go.sum","go.mod"],
      "fileExt":".go"
    }
    ,
    {
      "lang": ".net",
      "depTool": "nuget",
      "fileNames": ["*.csproj"],
      "fileExt":".cs"
    }
  ]
}
