{
  "ignoreDirs": [".git"],
  "ignoreFiles": [".gitignore"],
  "depManagers": [
    {
      "lang": "Java",
      "depTool": "maven",
      "fileName": "pom.xml",
      "fileExt": ".java"
    },
    {
      "lang": "Java",
      "depTool": "gradle",
      "fileName": "build.gradle",
      "fileExt": ".java"
    },
    {
      "lang": "JavaScript",
      "depTool": "npm",
      "fileName": "package-lock.json",
      "fileExt": ".js"
    },
    {
      "lang": "Python",
      "depTool": "pip",
      "fileName": "requirements",
      "fileExt": ".py"
    },
    {
      "lang": "Python",
      "depTool": "pipenv",
      "fileName": "Pipfile.lock",
      "fileExt": ".py"
    },
    {
      "lang": "Python",
      "depTool": "poetry",
      "fileName": "poetry.lock",
       "fileExt":".py"
    },
    {
      "lang": "Go",
      "depTool": "mod",
      "fileName": "go.sum",
      "fileExt":".go"
    }
  ]
}
